		"@unocss/preset-uno": "^0.55.7",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"unocss": "^0.57.2",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.3.1",

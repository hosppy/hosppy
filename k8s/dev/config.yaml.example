apiVersion: v1
kind: ConfigMap
metadata:
  name: hosppy-configmap
  namespace: hosppy-system
data:
  SPRING_PROFILES_ACTIVE: test
  SERVER_PORT: "80"
  SIGNING_SECRET: your_signing_secret
  WEB_DOMAIN: your_web_domain
  EMAIL_SERVICE_ENDPOINT: http://email-service
  ACCOUNT_SERVICE_ENDPOINT: http://account-service
  ACCOUNT_DATASOURCE_URL: jdbc:mysql://mysql-svc/hosppy_account?useUnicode=true&characterEncoding=utf-8
  ACCOUNT_DATASOURCE_USERNAME: root
  ACCOUNT_DATASOURCE_PASSWORD: root
  ALIYUN_ACCESS_KEY: YOUR_ALIYUN_ACCESS_KEY
  ALIYUN_ACCESS_SECRET: YOUR_ALIYUN_ACCESS_SECRET
  MAIL_FROM: hosppy@mail.hosppy.cn
  MAIL_FROM_NAME: hosppy